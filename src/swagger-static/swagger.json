                        },
                        "name": {
                          "type": "string"
                        }
                      }
                    }
                  },
                  "required": [
                    "id",
                    "isActive",
                    "created_at",
                    "updated_at",
                    "username",
                    "phone",
                    "email",
                    "balance",
                    "role"
